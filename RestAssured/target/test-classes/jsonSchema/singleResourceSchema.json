{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "data",
    "support"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "required": [
        "id",
        "name",
        "year",
        "color",
        "pantone_value"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/data/properties/id",
          "type": "integer"
        },
        "name": {
          "$id": "#/properties/data/properties/name",
          "type": "string"
        },
        "year": {
          "$id": "#/properties/data/properties/year",
          "type": "integer"
        },
        "color": {
          "$id": "#/properties/data/properties/color",
          "type": "string"
        },
        "pantone_value": {
          "$id": "#/properties/data/properties/pantone_value",
          "type": "string"
        }
      },
      "additionalProperties": true
    },
    "support": {
      "$id": "#/properties/support",
      "type": "object",
      "required": [
        "url",
        "text"
      ],
      "properties": {
        "url": {
          "$id": "#/properties/support/properties/url",
          "type": "string"
        },
        "text": {
          "$id": "#/properties/support/properties/text",
          "type": "string"
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}